                                                    <div class="input-group input-group-sm mb-3">
                                                        <span class="input-group-text" id="searchRequestIcon"
                                                            style="cursor: pointer;">
                                                            <i class="bi bi-search"></i>
                                                        </span>
                                                        <input type="text" class="form-control"
                                                            aria-label="search request" aria-describedby="searchRequest"
                                                            placeholder="Cari berdasarkan Nama Coa" id="searchRequest"
                                                            oninput="handleSearchCoa(event)">
                                                    </div>

                                                    <div class="tableNoFixHead">
                                                        <table class="table table-striped table-hover"
                                                            style="width:100%;text-align: center;font-size: 16px;"
                                                            id="alldattab">

                                                            <thead>
                                                                <tr>
                                                                    <th scope="col"
                                                                        style="text-align: center; vertical-align: middle; width: 5%;">
                                                                        No</th>
                                                                    <th scope="col"
                                                                        style="text-align: left; vertical-align: middle; width: 7%;">
                                                                        Kode</th>
                                                                    <th scope="col"
                                                                        style="text-align: left; vertical-align: middle; width: 20%;">
                                                                        Nama Plan</th>
                                                                    <th scope="col"
                                                                        style="text-align: left; vertical-align: middle; width: 30%;">
                                                                        keterangan</th>
                                                                    <th scope="col"
                                                                        style="text-align: left; vertical-align: middle; width: 30%;">
                                                                        Note</th>
                                                                    <th scope="col"
                                                                        style="text-align: center; vertical-align: middle; width: 5%;">
                                                                        Aksi</th>
                                                                </tr>
                                                            </thead>

                                                            <tbody id="divhpsdata">
                                                                <!-- baris data akan di-inject JS -->
                                                            </tbody>

                                                            <!-- baris untuk pesan "tidak ditemukan" -->
                                                            <tbody id="datatidakditemukan" style="display: none;">
                                                                <tr>
                                                                    <td colspan="6"
                                                                        style="text-align: center; font-style: italic; color: #888;">
                                                                        Data tidak ditemukan
                                                                    </td>
                                                                </tr>
                                                            </tbody>

                                                        </table>
                                                    </div>


                                                    <div style="text-align: center;display: none;" id="load-more-btn">
                                                        <i class="bi bi-caret-down-fill" id="load-more-btn-icon"
                                                            style="cursor: pointer;" onclick="loadMoreData()"></i>
                                                    </div>
                                            </div>
                                        </div>
                                    </main>


                                    </div>


                                    <%- include('layout/nav-loading') %>

