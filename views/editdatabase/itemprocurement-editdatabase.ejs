                                                            <th scope="col" style="text-align: left; vertical-align: middle; width: 20%;" class="d-none">Gambar</th>
                                                            <th scope="col" style="text-align: center; vertical-align: middle; width: 5%;">Aksi</th>
                                                        </tr>
                                                    </thead>
                                            
                                                    <tbody id="divhpsdata"></tbody>
                                            
                                                    <!-- Pesan data tidak ditemukan -->
                                                    <tbody id="datatidakditemukan" style="display: none;">
